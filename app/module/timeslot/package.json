{
    "title": "Timeslot",
    "name": "timeslot",
    "core": true,
    "icon": "timeslot:menu",
    "configEntryPoint": "repository.js",
    "entryPoint": {
        "name": "timeslot-index",
        "path": {
            "directory": "element/timeslot-index",
            "nameFile": "timeslot-index",
            "extension": "js"
        }
    },
    "autoloads": [
        {
            "name": "AbstractTimeslotService",
            "path": {
                "directory": "src",
                "nameFile": "AbstractTimeslotService",
                "extension": "js"
            }
        },
        {
            "name": "TimeslotService",
            "path": {
                "directory": "src",
                "nameFile": "TimeslotService",
                "extension": "js"
            }
        },
        {
            "name": "TimeslotEntity",
            "path": {
                "directory": "src/entity",
                "nameFile": "TimeslotEntity",
                "extension": "js"
            }
        },
        {
            "name": "Injector",
            "path": {
                "directory": "src/entity/embedded",
                "nameFile": "Injector",
                "extension": "js"
            }
        },
        {
            "name": "MongoTimeslotAdapter",
            "path": {
                "directory": "src/storage/adapter/mongo",
                "nameFile": "MongoTimeslotAdapter",
                "extension": "js"
            }
        },
        {
            "name": "DexieTimeslotAdapter",
            "path": {
                "directory": "src/storage/adapter/dexie",
                "nameFile": "DexieTimeslotAdapter",
                "extension": "js"
            }
        },
        {
            "name": "Test1",
            "path": {
                "directory": "src/injector",
                "nameFile": "Test1",
                "extension": "js"
            }
        },
        {
            "name": "Test2",
            "path": {
                "directory": "src/injector",
                "nameFile": "Test2",
                "extension": "js"
            }
        }
    ],
    "autoloadsWc": [
        {
            "name": "timeslot-icons",
            "path": {
                "directory": "element/timeslot-icons",
                "nameFile": "timeslot-icons",
                "extension": "js"
            }
        },
        {
            "name": "timeslot-style",
            "path": {
                "directory": "element/timeslot-style",
                "nameFile": "timeslot-style",
                "extension": "js"
            }
        }
    ],
    "widgets": [
        {
            "name": "Timeslot tags",
            "description": "Widget to view tagged timeslot",
            "property": "tags",
            "label": "tags",
            "require": true,
            "webComponent": {
                "name": "paper-timeslot-tags",
                "path": {
                    "directory": "timeslot/element/widget/paper-timeslot-tags",
                    "nameFile": "paper-timeslot-tags",
                    "extension": "js"
                }
            },
            "webComponentData": {
                "name": "paper-timeslot-tags-data",
                "path": {
                    "directory": "timeslot/element/widget/paper-timeslot-tags",
                    "nameFile": "paper-timeslot-tags-data",
                    "extension": "js"
                }
            }
        },
        {
            "name": "Timeslot monitors",
            "description": "Widget to view the timeslots assiate to the montors",
            "property": "monitors",
            "label": "monitors",
            "require": true,
            "webComponent": {
                "name": "paper-timeslot-monitors",
                "path": {
                    "directory": "timeslot/element/widget/paper-timeslot-monitors",
                    "nameFile": "paper-timeslot-monitors",
                    "extension": "js"
                }
            },
            "webComponentData": {
                "name": "paper-timeslot-monitors-data",
                "path": {
                    "directory": "timeslot/element/widget/paper-timeslot-monitors",
                    "nameFile": "paper-timeslot-monitors-data",
                    "extension": "js"
                }
            }
